{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/examination",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/examination",
      "destination": "/"
    },
    {
      "source": "/examination/(.*)",
      "destination": "/$1"
    }
  ]
}
